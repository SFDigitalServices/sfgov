{
  "name": "sfgov_admin",
  "proxy": "https://sfgov.lndo.site",
  "autoprefixer": {
    "browsers": [
      "last 2 versions"
    ]
  },
  "browsersync": {
    "files": [
      "../css/**/*.css",
      {
        "match": [
          "../**/*.module",
          "../**/*.php",
          "../**/*.inc",
          "../**/*.twig",
          "../*.yml"
        ]
      }
    ]
  },
  "sass": {
    "outputStyle": "expanded"
  }
}
