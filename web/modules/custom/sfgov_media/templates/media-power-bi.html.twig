{#
/**
 * @file
 * Default theme implementation to embed a Power BI chart.
 *
 * Available variables:
 * - width: The width of the iframe.
 * - height: The height of the iframe.
 *
 * @ingroup themeable
 */
#}
{{ attach_library("sfgov_media/power_bi") }}
<div data-power-bi data-device="{{ device }}" data-src="{{ url }}" style="display: none">
  <iframe width="{{ width }}" height="{{ height }}" loading="lazy"></iframe>
</div>
