<?php

/**
 * @file
 * Contains sfgov_qless.module.
 */

/**
 * Implements hook_preprocess_HOOK().
 */
function sfgov_qless_preprocess_node(&$variables) {
  if ($variables['node']->id() == '1699') {
    $values = \Drupal::service('sfgov_qless');
    $variables['qless'] = $values->renderTable();
  }
}
