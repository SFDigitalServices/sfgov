{#
/**
 * @file
 * Template for ECK location entity, Address (physical) bundle, rendered with
 * location_page view mode. This is used on 'location' nodes, specifically the
 * "At %department* locations, referenced from field_departments.
 */
#}

{% set parent = eck_entity.getParentEntity() %}
{% set classes = [
  'eck-entity',
  'sfgov-address',
  'sfgov-address--physical',
  'sfgov-address--physical--location-page',
] %}

<div{{ attributes.addClass(classes) }}>
  {{ content }}
</div>
