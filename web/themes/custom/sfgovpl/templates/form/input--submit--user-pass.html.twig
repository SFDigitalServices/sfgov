{#
/**
 * @file
 * Theme override for an 'input' #type form element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - children: Optional additional rendered elements.
 *
 * @see template_preprocess_input()
 */
#}
<input{{ attributes.addClass(['btn', 'font-inherit', 'w-auto']) }} />{{ children }}
<a href="/user/sfgov-login" data-drupal-selector="edit-cancel" class="btn btn-secondary w-auto">{{ 'Cancel'|t }}</a>
