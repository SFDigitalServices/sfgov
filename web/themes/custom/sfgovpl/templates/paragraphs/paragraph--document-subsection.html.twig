{#
/**
 * @file
 * Theme override for Document Subsection paragraph.
 *
 * @see paragraph.html.twig
 */
#}
<div class="paragraph--type--document-subsection">
  {% if paragraph.field_title is not null %}
    <h3 class="title-4">
      {{ paragraph.field_title.value }}
    </h3>
  {% endif %}
  {{ content|without('field_title') }}
</div>
