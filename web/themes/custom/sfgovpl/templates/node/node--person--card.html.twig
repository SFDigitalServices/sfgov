{% extends 'node.html.twig' %}

{% block title %}
{% endblock title %}

{% block node %}
  {% block body %}
    {% include '@theme/person-card.twig' with {
      "url": url,
      "first_name": node.field_first_name.value,
      "last_name": node.field_last_name.value,
      "position_title": node.field_title.value,
      "position_subtitle": node.field_sub_title.value,
      "photo": file_url(node.field_photo.entity.fileuri)
    } %}
  {% endblock body %}
{% endblock node %}
