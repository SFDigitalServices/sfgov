<div class="sfgov-footer">
  <div class="sfgov-footer__container">
    <div class="sfgov-footer__top">
      <div class="usa-nav-container">
        {% block navigation %}
          {% include 'molecules-footer-navigation' %}
        {% endblock %}
      </div>
    </div>
    <div class="sfgov-footer__bottom">
      <div class="usa-nav-container">
        <span class="logo">
          <img src="{{ footer.image_url }}" alt="{{ footer.image_alt }}"/>
        </span>
        <span class="text">
          {{ footer.text }}
        </span>
      </div>
    </div>
  </div>
</div>
