{
  "name": "components",
  "browserSyncProxy" : "https://sfgov-retheme.lndo.site/",
  "bowerDir": "./bower_components",
  "css": {
    "file" : "src/sass/drupal.scss",
    "src": [
      "src/sass/**/*.scss",
      "src/sass/*.scss"
    ],
    "dest": "dist/css",
    "includePaths": []
  },
  "autoprefixer": [
    "last 2 versions",
    "> 1%",
    "ie 11"
  ],
  "sass": {
    "outputStyle": "expanded"
  }
}
