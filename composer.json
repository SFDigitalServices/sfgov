        "drupal/jsonapi_extras": "^3.23",
        "drupal/key": "^1.15",
        "drupal/key_value_field": "^1.2",
        "drupal/link_attributes": "^2.1",
        "drupal/linkit": "^6.0",
        "drupal/lionbridge_translation_provider": "^2.23",
        "drupal/maxlength": "dev-3350059-remove-ckeditor-4",
        "drupal/media_bulk_upload": "^3.0",
        "drupal/media_entity_download": "^2.2",
        "drupal/media_entity_file_replace": "^1.1",
        "drupal/media_file_delete": "^1.2",
        "drupal/media_revisions_ui": "^2.0",
        "drupal/metatag": "^1.23",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/multiple_fields_remove_button": "^2.2",
        "drupal/node_edit_protection": "^1.1",
        "drupal/office_hours": "^1.1",
        "drupal/pantheon_advanced_page_cache": "^2.2",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.6",
        "drupal/permissions_filter": "^1.2",
        "drupal/protected_pages": "^1.1",
        "drupal/public_preview": "^1.0@beta",
        "drupal/quick_node_clone": "^1.14",
        "drupal/redirect": "^1.2",
        "drupal/rest_views": "^3.0@alpha",
        "drupal/restui": "^1.18",
        "drupal/role_delegation": "^1.1",
        "drupal/scheduler": "^2.0",
        "drupal/scheduler_content_moderation_integration": "^2.0@beta",
        "drupal/search_api": "^1.31",
        "drupal/search_api_pantheon": "^8.1",
        "drupal/search_api_solr": "^4.3",
        "drupal/search_api_solr_legacy": "^4.2",
        "drupal/sendgrid_integration": "^2.1",
        "drupal/simple_block": "^1.0@beta",
        "drupal/simple_gmap": "^3.0",
        "drupal/simple_media_bulk_upload": "^1.0",
        "drupal/simple_sitemap": "^4.1",
        "drupal/simplesamlphp_auth": "^4.0",
        "drupal/smart_date": "^4.1@RC",
        "drupal/smart_trim": "^2.0",
        "drupal/stable": "^2.0",
        "drupal/subpathauto": "^1.1",
        "drupal/telephone_formatter": "^1.2",
        "drupal/telephone_validation": "^2.4",
        "drupal/tmgmt": "^1.12",
        "drupal/tmgmt_google": "^1.0",
        "drupal/tmgmt_xtm": "^6.1",
        "drupal/toc_api": "^1.0",
        "drupal/toc_filter": "^2.0",
        "drupal/token": "1.12",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/variationcache": "^1.0",
        "drupal/video_embed_field": "^2.4",
        "drupal/viewfield": "^3.0@alpha",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_conditional": "^1.0",
        "drupal/views_data_export": "^1.1",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/webform": "^6.2@RC",
        "drush-ops/behat-drush-endpoint": "^0.0.5",
        "drush/drush": "^12",
        "enyo/dropzone": "^5.7",
        "google/apiclient": "~2.0",
        "joachim-n/composer-manifest": "^1.0",
        "jsanahuja/jquery.instagramfeed": "^1.2",
        "mglaman/composer-drupal-lenient": "^1.0",
        "oomphinc/composer-installers-extender": "^2",
        "pantheon-systems/drupal-integrations": "^8.0",
        "pantheon-systems/quicksilver-pushback": "^2",
        "phpoffice/phpword": "^0.18.2",
        "rvtraveller/qs-composer-installer": "^1.1",
        "simplesamlphp/composer-module-installer": "^1.3",
        "vikpe/php-html-heading-normalizer": "^1.1",
        "wikimedia/composer-merge-plugin": "2.0.1",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "drupal/core-dev": "^10",
        "drupal/devel": "^5.0",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/drupal-extension": "^5",
        "drupal/stage_file_proxy": "^2.1",
        "phpstan/phpstan": "^1.1",
        "phpunit/phpunit": "^7 | ^8 | ^9 | ^10"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "scripts/composer/SFGovSamlHandler.php"
        ]
    },
    "scripts": {
        "build-assets": [
            "@prepare-for-pantheon",
            "composer install --optimize-autoloader"
        ],
        "lint": "find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": "echo 'No code sniff step defined.'",
        "unit-test": "echo 'No unit test step defined.'",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "prepare-for-pantheon": "DrupalProject\\composer\\ScriptHandler::prepareForPantheon",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "SFGovSaml\\SFGovSamlHandler::copyDependencies"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "SFGovSaml\\SFGovSamlHandler::copyDependencies"
        ],
        "post-create-project-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-drupal-scaffold-cmd": []
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "build-env": {
            "install-cms": [
                "drush site-install standard --account-mail={account-mail} --account-name={account-name} --account-pass={account-pass} --site-mail={site-mail} --site-name={site-name} --yes",
                "drush pm-enable config_direct_save simple_block --yes",
                "drush pm-uninstall block_content --yes"
            ],
            "export-configuration": "drush config-export --yes"
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "excludes": [
                "robots.txt"
            ],
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[project-root]/.editorconfig": false
            }
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/dropzonejs/composer.libraries.json"
            ]
        },
        "patches": {
            "drupal/quick_node_clone": {
                "Remove translations when cloning nodes": "https://www.drupal.org/files/issues/2021-05-13/remove_translations.patch",
                "Node clone does not clone the most recent revision": "https://www.drupal.org/files/issues/2021-08-03/get-correct-revision-3226576-2.patch"
            },
            "drupal/telephone_validation": {
                "Allow emergency and short codes.": "https://www.drupal.org/files/issues/2020-04-07/allow_emergency-2976959-6.patch"
            },
            "drupal/g_translate": {
                "Adds a new option to display links": "patches/gtranslate_sfgov_links.patch"
            },
            "drupal/core": {
                "Entity links ignore language negotiation API": "https://www.drupal.org/files/issues/2023-12-18/3061761-57.patch",
                "StringFormatter generates links in wrong language when linking to entity": "patches/stringformatter-ignores-language-negotiation-2648288.patch",
                "Use hook_theme_suggestions in views": "https://www.drupal.org/files/issues/2023-06-12/2923634-92.patch",
                "Node local tasks tabs do not appear on node revisions": "https://www.drupal.org/files/issues/2019-11-12/drupal-node_revision_local_tasks_canonical-2885278-15.patch",
                "Invalid config structures can result in exceptions when saving a config entity": "https://www.drupal.org/files/issues/2021-11-29/2925890-29.patch",
                "Allow tokens in entity reference views selection arguments": "https://www.drupal.org/files/issues/2023-09-28/1699378-254.patch",
                "User can't reference unpublished content even when they have access to it": "https://www.drupal.org/files/issues/2024-02-13/2845144-87.patch",
                "Notice: Undefined index: #type in Drupal\\Core\\Form\\FormHelper::processStates()": "https://www.drupal.org/files/issues/2021-05-12/2700667-133a.patch",
                "TranslatableMarkup for Invalid Argument Exception: $string (Array) must be a string.": "https://www.drupal.org/files/issues/2019-04-01/2719553-26-null_for_string.patch",
                "PHP8.1 deprecation: str_replace(): Passing null to parameter #3": "https://www.drupal.org/files/issues/2023-06-07/3294680-22.patch",
                "FieldUiTable preRender undefined values causing an error on entity manage form display page.": "patches/undefined-values-in-fielduitable-prerender.patch"
            },
            "drupal/amplitude": {
                "Page scroll depth": "patches/amplitude-scroll-depth.patch",
                "JSON parse event property values": "patches/amplitude-parse-event-property-values.patch",
                "Auto-capture href for anchors": "patches/auto-capture-anchor-href.patch"
            },
            "drupal/block_field": {
                "Error when updating block fields": "https://www.drupal.org/files/issues/2022-08-08/block_field-available-blocks-2861670-56.patch"
            },
            "drupal/datalayer": {
                "Update field output to always be an array to allow for multivalue fields": "https://www.drupal.org/files/issues/2023-03-01/2940811-update-field-output-to-array-21.patch"
            },
            "drupal/entity_browser": {
                "Delay entity browser modal positioning": "patches/entity_browser-sfgov-modal-position.patch"
            },
            "drupal/public_preview": {
                "Public Preview is Registered as an Admin Route Due to _node_operation_route Usage": "https://www.drupal.org/files/issues/2020-12-22/3189531-node-operation-route.patch"
            },
            "drupal/form_process_states": {
                "Undefined index error on form elements without type set": "https://www.drupal.org/files/issues/2021-05-12/2700667-133b.patch"
            },
            "drupal/entity_reference_unpublished": {
                "Allow reference to unpublished media": "patches/entity_reference_unpublished-media.patch"
            },
            "drupal/user_email_textformat": {
                "Automated Drupal 9 compatibility fixes": "https://www.drupal.org/files/issues/2020-06-06/user_email_textformat.1.5.rector.patch"
            },
            "drupal/media_entity_file_replace": {
                "Multi value field file replace": "https://www.drupal.org/files/issues/2023-11-29/3254763-9.patch",
                "Labels and descriptions update": "patches/media_entity_file_replace--update-labels.patch"
            },
            "drupal/views_data_export": {
                "Batch mode does not allow other modules to alter query": "https://www.drupal.org/files/issues/2021-02-12/3173296-9-batch-query-alter.patch"
            },
            "drupal/link_attributes": {
                "Add Aria Labelledby": "https://www.drupal.org/files/issues/2023-04-06/3352685-add-aria-labelledby.patch"
            },
            "drupal/simple_sitemap": {
                "Error when running update script to upgrade from 3.11 to 4.1.4": "https://www.drupal.org/files/issues/2023-07-30/3377976-3.patch"
            },
            "drupal/content_lock": {
                "Fix PHP 8.2+ deprecation notices": "https://www.drupal.org/files/issues/2023-05-22/3343964-5.patch"
            },
            "drupal/linkit": {
                "Linkit Dialog list has no background color": "https://www.drupal.org/files/issues/2024-02-07/linkit-relative-3381766-13.patch"
            }
        },
        "enable-patching": "true",
        "drupal-lenient": {
            "allowed-list": ["anonymous_redirect"]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "simplesamlphp/composer-module-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "joachim-n/composer-manifest": true,
            "oomphinc/composer-installers-extender": true,
            "rvtraveller/qs-composer-installer": true,
            "zaporylie/composer-drupal-optimizations": true,
            "wikimedia/composer-merge-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "mglaman/composer-drupal-lenient": true
        }
    }
}
