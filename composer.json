{
    "name": "sfdigitalservices/sfgov",
    "description": "Install drops-8 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": [
                "drupal/anonymous_redirect",
                "drupal/maxlength"
            ]
        },
        "drupal/anonymous_redirect": {
            "type": "git",
            "url": "https://git.drupalcode.org/issue/anonymous_redirect-3286076.git"
        },
        "drupal/maxlength": {
            "type": "git",
            "url": "https://git.drupalcode.org/issue/maxlength-3350059.git"
        },
        "jqueryinstagramfeed": {
            "type": "package",
            "package": {
                "name": "jsanahuja/jquery.instagramfeed",
                "version": "1.2.5",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/jsanahuja/jquery.instagramFeed.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "php": ">=8.1",
        "composer/installers": "^1.9",
