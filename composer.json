        "drupal/dropzonejs": "^2.7",
        "drupal/eck": "dev-1.x",
        "drupal/embed": "^1.4",
        "drupal/entity": "^1.2",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_reference_unpublished": "^2.0",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/exif_orientation": "^1.1",
